<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sonai.portfolio.MainUI">

    <EditText
        android:id="@+id/firstName"
        android:layout_width="209dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="First Name"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="38dp" />

    <EditText
        android:id="@+id/surname"
        android:layout_width="209dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Surname"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="106dp" />

    <EditText
        android:id="@+id/maidenName"
        android:layout_width="209dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Maiden Name"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="182dp" />

    <EditText
        android:id="@+id/birthplace"
        android:layout_width="208dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Birthplace"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="253dp" />

    <EditText
        android:id="@+id/favouriteBrand"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Favourtite Brand"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="322dp" />

    <TextView
        android:id="@+id/firstName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="First Name:"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="53dp" />

    <TextView
        android:id="@+id/surname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Surname:"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="125dp" />

    <TextView
        android:id="@+id/maidenName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maiden Name:"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="202dp" />

    <TextView
        android:id="@+id/birthplace"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Birthplace:"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="264dp" />

    <TextView
        android:id="@+id/favouriteBrand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Favourite Brand:"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="330dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Make me a jade!!"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteY="393dp" />
</android.support.constraint.ConstraintLayout>
